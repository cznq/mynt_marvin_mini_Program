page {
  background: #fff;
}

.page {
  flex-direction: column;
  padding: 0 60rpx;
}

.header {
  display: flex;
  width: 670rpx;
  height: 170rpx;
}

.header .t1 {
  margin: 40rpx 0;
  margin-right: 20rpx;
  background: wheat;
}

.header .t1 image {
  width: 90rpx;
  height: 90rpx;
  border-radius: 10rpx;
  background: rgba(255,255,255,1);
  box-shadow: 0 6px 92px 0 rgba(43,51,72,.12);
}

.header .t2 {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 750rpx;
  margin: 40rpx 0;
}

.header .t2 .top {
  line-height: 38rpx;
  display: flex;
  padding-top: 7rpx;
}

.header .t2 .top text {
  font-size: 36rpx;
  font-weight: 500;
  height: 34rpx;
  color: #1a2a4b;
}

.header .t2 .top view {
  font-size: 22rpx;
  margin-left: 32rpx;
  padding-right: 14rpx;
  padding-left: 13rpx;
  color: #1a2a4b;
  border: 1rpx solid #c6c9d4;
  border-radius: 7rpx;
}

.header .t2 .bottom {
  font-size: 24rpx;
  font-weight: 300;
  height: 24rpx;
  padding-bottom: 9rpx;
  color: #8891a9;
}

.content {
  width: 670rpx;
  background: #f5f6f7;
}

.footers {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 670rpx;
  height: 279rpx;
  margin: 0 40rpx;
}

.footers .foot-top {
  display: flex;
  justify-content: space-between;
  width: 670rpx;
  height: 98rpx;
  border-bottom: 1rpx dashed rgba(136,145,169,.3);
}

.footers .foot-top .d1 {
  align-content: space-between;
}

.footers .foot-top .d11 {
  font-size: 24rpx;
  font-weight: 400;
  line-height: 40rpx;
  color: #092344;
}

.footers .foot-top .d12 {
  font-size: 24rpx;
  font-weight: 400;
  line-height: 40rpx;
  color: #8891a9;
}

.footers .foot-top .d2 {
  font-size: 24rpx;
  font-weight: 400;
  line-height: 23px;
  display: flex;
  align-items: center;
  padding-bottom: 32rpx;
  color: #8891a9;
}

.footers .foot-top .d3 {
  text-align: right;
}

.footers .foot-top .d31 {
  font-family: NotoSansHans-Regular;
  font-size: 24rpx;
  font-weight: 400;
  line-height: 40rpx;
  color: #092344;
}

.footers .foot-top .d32 {
  font-family: NotoSansHans-Regular;
  font-size: 24rpx;
  font-weight: 400;
  line-height: 40rpx;
  color: #092344;
}
