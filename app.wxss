/**app.wxss**/
@import 'weui.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
#myMap {
  width: 100%;
  height: 180px;
}
.map-cover {
  height: 150px;
  width: 100%;
  background-color: transparent;
}
.weui-form-preview.last:after {
  border:none;
}